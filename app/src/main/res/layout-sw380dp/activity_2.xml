        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="@string/KidneyPanel"
        android:textColor="@color/textColorPrimary"
        android:textSize="24sp"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        app:fontFamily="@font/nunito_bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/grid_kidneytest"
        android:layout_below="@+id/kidneytest_text"
        android:padding="10dp">

        <Spinner
            android:id="@+id/spinner_kidneytest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/cardBackground" />

        <EditText
            android:id="@+id/enter_kidneytest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:maxLength="8"
            android:inputType="number|numberDecimal"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18sp"
            android:textColor="@color/textColorPrimary"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            android:onClick="show_cursor"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:onClick="onclick_kidney_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/rounded_corner"
            android:textColor="@color/textColorPrimary"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </LinearLayout>

    <ListView
        android:id="@+id/listview_kidneytest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_kidneytest"
        android:divider="@android:color/transparent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="12dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewLiverTest"
        android:layout_below="@+id/layoutCardViewKidneyTest"
        android:elevation="10dp"
        android:background="@drawable/rounded_corner_card"
        android:layout_marginTop="10dp"
        android:padding="3dp">

    <TextView
        android:id="@+id/livertest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="@string/LiverPanel"
        android:textColor="@color/textColorPrimary"
        android:textSize="24sp"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        app:fontFamily="@font/nunito_bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_livertest"
        android:orientation="horizontal"
        android:layout_below="@+id/livertest_text"
        android:padding="10dp">

        <Spinner
            android:id="@+id/spinner_livertest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/cardBackground" />

        <EditText
            android:id="@+id/enter_livertest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:maxLength="8"
            android:inputType="number|numberDecimal"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18sp"
            android:textColor="@color/textColorPrimary"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            android:onClick="show_cursor"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:onClick="onclick_liver_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/rounded_corner"
            android:textColor="@color/textColorPrimary"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </LinearLayout>

    <ListView
        android:id="@+id/listview_livertest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_livertest"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="12dp"
        android:divider="@android:color/transparent" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewElectrolytesTest"
        android:layout_below="@+id/layoutCardViewLiverTest"
        android:elevation="10dp"
        android:background="@drawable/rounded_corner_card"
        android:layout_marginTop="10dp"
        android:padding="3dp">
    <TextView
        android:id="@+id/electrolytestest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="@string/Electrolytes"
        android:textColor="@color/textColorPrimary"
        android:textSize="24sp"
        android:layout_marginTop="8dp"
        android:textStyle="bold"
        app:fontFamily="@font/nunito_bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_electrolytestest"
        android:orientation="horizontal"
        android:layout_below="@+id/electrolytestest_text"
        android:padding="10dp">

        <Spinner
            android:id="@+id/spinner_electrolytestest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/cardBackground"/>

        <EditText
            android:id="@+id/enter_electrolytestest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:inputType="number|numberDecimal"
            android:maxLength="8"
            android:onClick="show_cursor"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18sp"
            android:textColor="@color/textColorPrimary"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:onClick="onclick_electrolytes_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/rounded_corner"
            android:textColor="@color/textColorPrimary"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </LinearLayout>

    <ListView
        android:id="@+id/listview_electrolytestest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_electrolytestest"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="12dp"
        android:divider="@android:color/transparent" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewProteinTest"
        android:layout_below="@+id/layoutCardViewElectrolytesTest"
        android:elevation="10dp"
        android:background="@drawable/rounded_corner_card"
        android:layout_marginTop="10dp"
        android:padding="3dp">
    <TextView
        android:id="@+id/proteintest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:layout_marginTop="8dp"
        android:text="@string/Proteins"
        android:textColor="@color/textColorPrimary"
        android:textSize="24sp"
        android:textStyle="bold"
        app:fontFamily="@font/nunito_bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_proteintest"
        android:orientation="horizontal"
        android:layout_below="@+id/proteintest_text"
        android:padding="10dp">

        <Spinner
            android:id="@+id/spinner_proteintest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/cardBackground"/>

        <EditText
            android:id="@+id/enter_proteintest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:inputType="number|numberDecimal"
            android:maxLength="8"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18sp"
            android:textColor="@color/textColorPrimary"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            android:onClick="show_cursor"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:onClick="onclick_protein_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/rounded_corner"
            android:textColor="@color/textColorPrimary"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </LinearLayout>

    <ListView
        android:id="@+id/listview_proteintest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_proteintest"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="12dp"
        android:divider="@android:color/transparent" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewLipidTest"
        android:layout_below="@+id/layoutCardViewCBC"
        android:elevation="10dp"
        android:background="@drawable/rounded_corner_card"
        android:layout_marginTop="10dp"
        android:padding="3dp">
    <TextView
        android:id="@+id/lipidstest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="@string/LipidPanel"
        android:textColor="@color/textColorPrimary"
        android:textSize="24sp"
        android:layout_marginTop="8dp"
