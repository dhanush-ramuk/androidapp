            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/cardBackground"
           />

        <EditText
            android:id="@+id/enter_generaltest"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="5"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:inputType="number|numberDecimal"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18sp"
            android:textColor="@color/textColorPrimary"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            android:maxLength="8"
            android:onClick="show_cursor"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:onClick="onclick_general_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/rounded_corner"
            android:textColor="@color/textColorPrimary"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </LinearLayout>

    <ListView
        android:id="@+id/listview_generaltest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_generaltest"
        android:divider="@android:color/transparent"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="12dp"
        android:isScrollContainer="false"/>
    </LinearLayout>
    <LinearLayout
