            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="3"
            android:onClick="onclick_kidney_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/button_rounded_corner"
            android:textColor="@color/labText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </GridLayout>

    <ListView
        android:id="@+id/listview_kidneytest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_kidneytest"
        android:layout_marginLeft="10dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="10.0sp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewLiverTest"
        android:layout_below="@+id/layoutCardViewKidneyTest"
        android:elevation="10dp"
        android:background="@color/labSecondary"
        android:layout_marginTop="5dp"
        android:layout_marginRight="2.5dp"
        android:layout_marginLeft="2.5dp">

    <TextView
        android:id="@+id/livertest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Liver panel"
        android:textColor="@color/labTextHeading"
        android:textSize="26sp"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        app:fontFamily="sans-serif-condensed" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_livertest"
        android:layout_below="@+id/livertest_text"
        android:padding="5dp">

        <Spinner
            android:id="@+id/spinner_livertest"
            style="@android:style/Widget.Material.Spinner"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/labSecondary"
            android:layout_row="0"
            android:layout_column="0" />

        <EditText
            android:id="@+id/enter_livertest"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_marginLeft="5dp"
            android:maxLength="8"
            android:inputType=""
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18dp"
            android:textColor="@color/labText"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="3"
            android:onClick="onclick_liver_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/button_rounded_corner"
            android:textColor="@color/labText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </GridLayout>

    <ListView
        android:id="@+id/listview_livertest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_livertest"
        android:layout_marginLeft="10dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="10.0sp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewElectrolytesTest"
        android:layout_below="@+id/layoutCardViewLiverTest"
        android:elevation="10dp"
        android:background="@color/labSecondary"
        android:layout_marginTop="5dp"
        android:layout_marginRight="2.5dp"
        android:layout_marginLeft="2.5dp">
    <TextView
        android:id="@+id/electrolytestest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Electrolytes"
        android:textColor="@color/labTextHeading"
        android:textSize="26sp"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        app:fontFamily="sans-serif-condensed" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_electrolytestest"
        android:layout_below="@+id/electrolytestest_text"
        android:padding="5dp">

        <Spinner
            android:id="@+id/spinner_electrolytestest"
            style="@android:style/Widget.Material.Spinner"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/labSecondary"/>

        <EditText
            android:id="@+id/enter_electrolytestest"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_marginLeft="5dp"
            android:inputType=""
            android:maxLength="8"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18dp"
            android:textColor="@color/labText"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="3"
            android:onClick="onclick_electrolytes_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/button_rounded_corner"
            android:textColor="@color/labText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </GridLayout>

    <ListView
        android:id="@+id/listview_electrolytestest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_electrolytestest"
        android:layout_marginLeft="10dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="10.0sp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewProteinTest"
        android:layout_below="@+id/layoutCardViewElectrolytesTest"
        android:elevation="10dp"
        android:background="@color/labSecondary"
        android:layout_marginTop="5dp"
        android:layout_marginRight="2.5dp"
        android:layout_marginLeft="2.5dp"
        android:layout_marginBottom="60dp">
    <TextView
        android:id="@+id/proteintest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Proteins"
        android:textColor="@color/labTextHeading"
        android:textSize="26sp"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        app:fontFamily="sans-serif-condensed" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_proteintest"
        android:layout_below="@+id/proteintest_text"
        android:padding="5dp">

        <Spinner
            android:id="@+id/spinner_proteintest"
            style="@android:style/Widget.Material.Spinner"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/labSecondary"/>

        <EditText
            android:id="@+id/enter_proteintest"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:layout_marginLeft="5dp"
            android:inputType=""
            android:maxLength="8"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:textSize="18dp"
            android:textColor="@color/labText"
            android:theme="@style/MyEditText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>

        <Button
            style="@style/Widget.AppCompat.Button"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="3"
            android:onClick="onclick_protein_test"
            android:text="@string/ok"
            android:textSize="18sp"
            android:background="@drawable/button_rounded_corner"
            android:textColor="@color/labText"
            android:textStyle="bold"
            app:fontFamily="sans-serif-smallcaps"/>
    </GridLayout>

    <ListView
        android:id="@+id/listview_proteintest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/grid_proteintest"
        android:layout_marginLeft="10dp"
        android:divider="@android:color/transparent"
        android:dividerHeight="10.0sp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/layoutCardViewLipidTest"
        android:layout_below="@+id/layoutCardViewCBC"
        android:elevation="10dp"
        android:background="@color/labSecondary"
        android:layout_marginTop="5dp"
        android:layout_marginRight="2.5dp"
        android:layout_marginLeft="2.5dp">
    <TextView
        android:id="@+id/lipidstest_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Lipid panel"
        android:textColor="@color/labTextHeading"
        android:textSize="26sp"
        android:layout_marginTop="10dp"
        android:textStyle="bold"
        app:fontFamily="sans-serif-condensed" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/grid_lipidstest"
        android:layout_below="@+id/lipidstest_text"
        android:padding="5dp">

        <Spinner
            android:id="@+id/spinner_lipidstest"
            style="@android:style/Widget.Material.Spinner"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:background="@drawable/spinner_background"
            android:popupBackground="@color/labSecondary"/>

        <EditText
            android:id="@+id/enter_lipidstest"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:maxLength="8"
            android:layout_marginLeft="5dp"
            android:inputType=""
